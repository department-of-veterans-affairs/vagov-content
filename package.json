{
  "name": "vagov-content",
  "version": "0.0.1",
  "description": "Content files for VA.gov",
  "engines": {
    "node": "8.10.0"
  },
  "scripts": {
    "clean": "rm -rf node_modules && rm package-lock.json",
    "build": "npm --prefix node_modules/vets-website run build -- --content-deployment --content-directory=${PWD}/pages --entry static-pages --brand-consolidation-enabled",
    "rebuild": "npm run clean && npm install && npm run build",
    "start": "npm --prefix node_modules/vets-website run heroku-serve build/localhost",
    "heroku-postbuild": "INSTALL_HOOKS=no npm run build",
    "temp": "does nothing"
  },
  "dependencies": {
    "vets-website": "git+https://github.com/department-of-veterans-affairs/vets-website.git#ns-14607"
  }
}
