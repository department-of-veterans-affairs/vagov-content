{
  "name": "vagov-content",
  "version": "0.0.1",
  "description": "Content files for VA.gov",
  "engines": {
    "node": "8.10.0"
  },
  "scripts": {
    "heroku-postbuild": "npm --prefix node_modules/vets-website run build -- --entry static-pages --brand-consolidation-enabled --content-directory=${PWD}/pages",
    "start": "npm --prefix node_modules/vets-website run heroku-serve build/localhost"
  },
  "dependencies": {
    "vets-website": "git+https://github.com/department-of-veterans-affairs/vets-website.git#ns-14607"
  }
}
