{
  "name": "vagov-content",
  "version": "0.0.1",
  "description": "Content files for VA.gov",
  "engines": {
    "node": "8.10.0",
    "yarn": "1.5.1"
  },
  "scripts": {
    "install": "cd ../vagov-apps && yarn install",
    "heroku-prebuild": "git clone --single-branch interim-cms --depth 1 https://github.com/department-of-veterans-affairs/vets-website ../vagov-apps",
    "heroku-postbuild": "cd ../vagov-apps && npm run heroku-postbuild"
  }
}
